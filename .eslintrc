{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-namespace": 0,
        "@typescript-eslint/ban-ts-comment": 0
    }

}
